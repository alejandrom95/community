	<div class = "container text-center">
		<header id="heading">		
			<img src="images/temp.jpg" class="img-circle" alt="Cinque Terre" width="304" height="236"> 
			<h2>User Name</h2>
		</header>
	</div>
	<div class="container-fluid">
		<div class="well"><h2>Events</h2></div>
		<div class="row">	
		    <!-- might have to change this to col-*-* -->
			<div class="col-sm-4" sytle="background-color:lavender;">
				<h4>Upcoming Events</h4>        
				  <table class="table table-bordered">
					<thead>
					  <tr>
						<th>Event</th>
						<th>Date</th>
						<th>Location</th>					
					  </tr>
					</thead>
					<tbody>
					  <tr>
						<td>St. Mary's Soup Kitchen</td>
						<td>4/3/17</td>
						<td>102 e 63rd St, Long Beach, CA 90805</td>
					  </tr>
					  <tr>
						<td>Lee elementary Bake Sale</td>
						<td>4/21/17</td>
						<td>1620 Temple Ave, Long Beach, CA 90804</td>
					  </tr>
					  <tr>
						<td>St Jude Toy Drive</td>
						<td>5/12/17</td>
						<td>1526 Del Amo Blvd, Long Beach, CA 90805</td>
					  </tr>
					</tbody>
				  </table>
				<button type="button" class="btn btn-default">More Events</button>
			</div><!--end of span 4 -->
			<div class ="col-sm-8" style = "background-color:white;">
				<h4>Local Events {{zipcode}}</h4> 

				<table class="table table-hover">
					<thead>
					  <tr>
						<th>Event</th>
						<th>Date</th>
						<th>Location</th>
						<th>Zipcode</th>
						<th>Request to Join</th>
					  </tr>
					</thead>
					<tbody>
					  {{#events}} 
					  <tr>
						<td>{{event_name}}</td>
						<td>{{date_time}}</td>
						<td>{{location}}</td>
						<td>{{zipcode}}</td>
						<td><button type="button" class="btn btn-default">+</button></td>
					  </tr>
					  {{/events}}

					</tbody>
				  </table><!-- end of table -->
				  {{#events}} 

					<li>{{event_id}}</li>
					<li>{{event_name}}</li>
					<li>{{owner_email}}</li>
					<li>{{description}}</li>
					<li>{{status}}</li>
					{{/events}}
				<ul class="pager">
					<li><a href="#">Previous</a></li>
					<li><a href="#">Next</a></li>
				</ul>
			</div><!--end of span 4-->
		</div><!-- end or row --> 
	</div><!--end of container div-->
	
	<div id="footer">
		<div class="container">
        <p>community</p>
        <p>Building Stronger Relationships With Our Neighbors.</p>
        
      </div>
	</div><!-- end footer -->
